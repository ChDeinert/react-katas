{
  "name": "react-katas",
  "version": "0.4.0",
  "description": "Different React components I created to improve myself",
  "author": "ChDeinert <ChDeinert@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:ChDeinert/react-katas.git"
  },
  "scripts": {
    "start": "start-storybook -p 8081",
    "test": "jest",
    "testwatch": "jest --watch",
    "prettify": "prettier --single-quote --tab-width 2 --trailing-comma es5 --write \"src/**/*.js\" ",
    "gh-pages:clean": "rimraf _gh-pages",
    "gh-pages:build": "build-storybook -o _gh-pages",
    "gh-pages:publish": "git-directory-deploy --directory _gh-pages",
    "gh-pages": "npm run gh-pages:clean && npm run gh-pages:build && npm run gh-pages:publish"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.8.2",
    "git-directory-deploy": "^1.5.1",
    "jest": "^20.0.1",
    "node-sass": "^4.5.2",
    "prettier": "^1.3.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5"
  },
  "peerDependencies": {
    "react": "^15.5.0",
    "react-dom": "^15.5.0"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ]
  },
  "dependencies": {
    "shortid": "^2.2.8"
  }
}
